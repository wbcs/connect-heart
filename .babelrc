{
  "presets": ["@babel/preset-typescript", "@babel/preset-react"],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    "@babel/plugin-syntax-dynamic-import",
    ["styled-jsx/babel", {
      "optimizeForSpeed": true
    }],
    ["babel-plugin-import", {
      "libraryName": "antd",
      "style": "css"
    }]
  ]
}