{

  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "react"],
  "env": {
    "browser": true,
    "node": true,
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "no-useless-constructor": 2,
    "no-useless-rename": 2,
    "no-var": 2,
    "one-var": [2, {
      "let": "never",
      "const": "never"
    }],
    "sort-keys": 1,
    "prefer-destructuring": 2,
    "eqeqeq": 2,
    "for-direction": 2,
    "no-unused-vars": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2
  }
}