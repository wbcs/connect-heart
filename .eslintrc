{

  "parser": "@typescript-eslint/parser", //定义ESLint的解析器
  "plugins": ["@typescript-eslint"], //定义了该eslint文件所依赖的插件
  "env": { //指定代码的运行环境
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "no-useless-constructor": 2,
    "no-useless-rename": 2,
    "no-var": 2,
    "one-var": [2, {
      "let": "never",
      "const": "never"
    }],
    "sort-keys": 1,
    "prefer-destructuring": 2
  }
}